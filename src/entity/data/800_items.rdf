    <ns1:geo>
      <rdf:Description rdf:about="http://geohash.org/xn76j4w13k2r">
        <ns1:longitude rdf:datatype="http://www.w3.org/2001/XMLSchema#double">139.798333</ns1:longitude>
        <ns1:latitude rdf:datatype="http://www.w3.org/2001/XMLSchema#double">35.521784</ns1:latitude>
