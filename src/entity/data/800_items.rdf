    <ns1:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/b/bf/Yasui_Fukai.jpg/220px-Yasui_Fukai.jpg"/>
    <ns3:sameAs rdf:resource="https://ja.wikipedia.org/wiki/深井英五"/>
  </ns2:Agent>
