  <ns2:Place rdf:about="https://nakamura196.github.io/repo/api/place/多摩川">
    <rdfs:label>多摩川</rdfs:label>
    <ns1:image rdf:resource="https://upload.wikimedia.org/wikipedia/commons/thumb/e/ee/%E5%A4%9A%E6%91%A9%E5%B7%9D%E3%81%AE%E8%A1%9B%E6%98%9F%E5%86%99%E7%9C%9F001.jpg/293px-%E5%A4%9A%E6%91%A9%E5%B7%9D%E3%81%AE%E8%A1%9B%E6%98%9F%E5%86%99%E7%9C%9F001.jpg"/>
